[Unit]
Description=Gunicorn service
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/cv-docker/
ExecStart=/usr/local/bin/gunicorn -t 2400 --preload --graceful-timeout 2400 --workers 33 -k eventlet --log-level debug --bind unix:cosmovis.sock -m 007 cosmoserv:app

[Install]
WantedBy=multi-user.target